{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
    "transform-node-env-inline"
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "livereactload/babel-transform",
            "imports": ["react"]
          }]
        }]
      ]
    }
  }
}
