@font-face {
  font-family:"<%= fontConfig.fontName %>";
  src:font-url("<%= fontConfig.fontName %>.eot");
  src:font-url("<%= fontConfig.fontName %>.eot#iefix") format("embedded-opentype"),
      font-url("<%= fontConfig.fontName %>.woff2") format("woff2"),
      font-url("<%= fontConfig.fontName %>.woff") format("woff"),
      font-url("<%= fontConfig.fontName %>.ttf") format("truetype");
}

.<%= fontConfig.fontShortName %>{
  use-font-<%= fontConfig.fontShortName %>();
}

<% for (var idx = 0; idx < glyphs.length; idx++) { %>
.<%= fontConfig.fontShortName %><%= fontConfig.bem?'-':'' %>-<%= glyphs[idx].name %>:before { content: $<%= fontConfig.fontShortName %>-var-<%= glyphs[idx].name %>;}<% } %>

